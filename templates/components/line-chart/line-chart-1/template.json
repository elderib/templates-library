{
  "type": "component.line-chart",
  "options": {
    "legend": {
      "isHidden": false
    },
    "chart": {
      "height": 400,
      "isAnimated": true
    },
    "yAxis": {
      "max": 65000,
      "min": 10000,
      "labels": {
        "format": {
          "numberStyle": "currency",
          "currency": "USD",
          "compactDisplay": "short",
          "notation": "compact"
        }
      }
    },
    "xAxis": {
      "tickAmount": 3,
      "categories": [
        "2019",
        "2020",
        "2021",
        "2022"
      ]
    },
    "plotOptions": {
      "series": {
        "pointWidth": 12
      }
    },
    "series": [
      {
        "data": "=@ctx.datasources.revenues",
        "name": "Revenues",
        "color": "positive",
        "dataLabels": [
          {
            "isEnabled": false
          }
        ]
      },
      {
        "data": "=@ctx.datasources.expenses",
        "name": "Expenses",
        "color": "negative",
        "dataLabels": [
          {
            "isEnabled": false
          }
        ]
      }
    ]
  }
}