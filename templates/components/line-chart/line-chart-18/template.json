{
  "options": {
    "chart": {
      "title": "Currency number style",
      "subtitle": "Notation: compact"
    },
    "plotOptions": {
      "series": {
        "pointWidth": 24
      }
    },
    "yAxis": {
      "tickAmount": 3,
      "labels": {
        "format": {
          "numberStyle": "currency",
          "currency": "USD",
          "notation": "compact"
        }
      },
      "max": 10000,
      "min": 0
    },
    "xAxis": {
      "categories": [
        "1",
        "2",
        "3",
        "4"
      ]
    },
    "series": [
      {
        "data": "=@ctx.datasources.line",
        "color": "color4"
      }
    ]
  }
}